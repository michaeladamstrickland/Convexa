############################################################
# Phase 4 API Environment Variable Template
# Copy this file to .env and enter your real API keys
############################################################

# ATTOM Property Data API
ATTOM_API_KEY=your_attom_api_key_here
ATTOM_BASE_URL=https://api.gateway.attomdata.com/propertyapi/v1.0.0

# BatchData Skip Trace API
BATCHDATA_API_KEY=your_batchdata_api_key_here
BATCHDATA_BASE_URL=https://api.batchdata.com/api

# Legacy APIs (Optional)
DATATREE_API_KEY=your_datatree_api_key_here
DATATREE_BASE_URL=https://api.datatree.com/api

# AI and Analytics
OPENAI_API_KEY=your_openai_api_key_here

# Maps and Geocoding
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Cost tracking and rate limiting
MAX_DAILY_API_COST_DOLLARS=100
ALERT_THRESHOLD_PERCENT=75
COST_TRACKING_ENABLED=true
MAX_BATCH_SKIP_TRACE_SIZE=10

# Rate limiting
ATTOM_CALLS_PER_SECOND=10
BATCHDATA_CALLS_PER_SECOND=5
OPENAI_CALLS_PER_MINUTE=60
# LeadFlow AI Empire - Environment Configuration

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4-turbo

# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/leadflow_ai"

# Twilio Configuration (for SMS/Voice)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_email_password

# Google Maps API
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Server Configuration
PORT=3000
NODE_ENV=development

# Intelligence Sources Configuration
LEGACY_SCRAPER_DELAY=2000
PROBATE_COURT_DELAY=3000
VIOLATION_SCRAPER_DELAY=1500
VACANCY_CHECK_DELAY=2500

# Revenue Optimization
MINIMUM_MOTIVATION_SCORE=75
MINIMUM_PROFIT_POTENTIAL=25000
AUTO_EXECUTION_ENABLED=true

# Logging
LOG_LEVEL=info
LOG_FILE=logs/leadflow-ai.log

# Artifacts & Storage (for /admin/artifacts and dialer storage)
ARTIFACT_SIGNING_SECRET=change_me
ARTIFACT_URL_TTL_SECONDS=86400
LOCAL_STORAGE_PATH=backend/run_storage
# STORAGE_BACKEND=local   # or 's3' when wired
# S3_BUCKET=
# S3_REGION=
