# =============================================================================
# APPLICATION CONFIGURATION
# =============================================================================

# Server Configuration
PORT=3001
NODE_ENV=production
CORS_ORIGIN=http://localhost:3000

# Security
JWT_SECRET=your_super_secure_jwt_secret_here_minimum_32_characters
ENCRYPTION_KEY=your_32_character_encryption_key_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs/leadflow.log

# Webhooks (Optional)
WEBHOOK_SECRET=your_webhook_secret_here
WEBHOOK_URL=https://your-app.com/webhooks/leadflow

# =============================================================================
# COST TRACKING & ALERTS
# =============================================================================

# Cost monitoring
COST_ALERT_THRESHOLD=80
COST_ALERT_EMAIL=your-email@domain.com
COST_ALERT_WEBHOOK=https://your-app.com/alerts/cost

# Daily limits
MAX_DAILY_COST=150
MAX_DAILY_CALLS=1000

# Weekly/Monthly limits
MAX_WEEKLY_COST=750
MAX_MONTHLY_COST=3000
BATCH_SKIP_TRACING_API_KEY=your-batch-skip-tracing-api-key
WHITEPAGES_PRO_API_KEY=your-whitepages-pro-api-key

# External APIs
DATATREE_API_KEY=your-datatree-api-key
ATTOM_API_KEY=your-attom-api-key
ESTATED_API_KEY=your-estated-api-key

# Scraping Configuration
PUPPETEER_HEADLESS=true
SCRAPER_DELAY_MIN=1000
SCRAPER_DELAY_MAX=3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Email (Optional - for notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000
