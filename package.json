{
  "name": "leadflow-ai-empire",
  "version": "1.0.0",
  "description": "Revolutionary Intelligence-Driven Real Estate Empire Builder",
  "type": "module",
  "main": "dist/server.js",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "tsx src/server.ts",
    "real-server": "tsx src/realServer.ts",
    "setup": "npx prisma generate && npx prisma migrate dev --name init && npx tsx prisma/seed.ts",
    "build": "tsc",
    "typecheck": "tsc --project tsconfig.server.json",
    "start": "cross-env BASIC_AUTH_USER=staging BASIC_AUTH_PASS=RockyDog456 tsx src/server.ts",
    "db:setup": "cross-env BASIC_AUTH_USER=staging BASIC_AUTH_PASS=RockyDog456 npx prisma migrate dev --name init && npx prisma generate && npx tsx prisma/seed.ts",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:reset": "npx prisma migrate reset --force",
    "db:generate": "npx prisma generate",
    "phase3": "tsc && node dist/phase3-launcher.js interactive",
    "empire-complete": "tsc && node dist/phase3-launcher.js complete",
    "empire-scaling": "tsc && node dist/phase3-launcher.js phase3",
    "empire-dashboard": "tsc && node dist/phase3-launcher.js dashboard",
    "empire-report": "tsc && node dist/phase3-launcher.js report",
    "empire-autonomous": "tsc && node dist/phase3-launcher.js autonomous",
    "empire-status": "tsc && node dist/phase3-launcher.js status",
    "test": "jest --detectOpenHandles",
    "test:unit": "vitest run --reporter=dot",
    "test:contract": "vitest run tests/contract --reporter=dot",
    "test:smoke": "vitest run tests/smoke --reporter=dot",
    "test:routes": "vitest run -t routes --reporter=default",
    "test:routes:pi1": "vitest run tests/routes/pi1-dialer.test.ts --reporter=default",
    "test:routes:import": "vitest run -t import --reporter=dot",
    "test:smoke:tiny": "node scripts/smoke/dialerTiny.mjs",
    "smoke:import": "node scripts/smoke/importCsv.mjs",
    "smoke:pi1:dialops": "node scripts/smoke/pi1DialOps.cjs",
    "smoke:pi1:campaigns": "node scripts/smoke/pi1Campaigns.cjs",
    "smoke:pi1:grades": "node scripts/smoke/pi1Grades.cjs",
    "smoke:pi1:all": "node scripts/smoke/runAllPI1.cjs",
    "smoke:pi2:campaigns": "node scripts/smoke/pi2Campaigns.cjs",
    "smoke:pi2:grades": "node scripts/smoke/pi2Grades.cjs",
    "smoke:pi2:stages": "node scripts/smoke/pi2Stages.cjs",
    "smoke:pi2:summary": "node scripts/smoke/pi2Summary.cjs",
    "smoke:pi2:export": "node scripts/smoke/pi2Export.cjs",
    "smoke:pi2:all": "node scripts/smoke/pi2Campaigns.cjs && node scripts/smoke/pi2Grades.cjs && node scripts/smoke/pi2Stages.cjs && node scripts/smoke/pi2Summary.cjs && node scripts/smoke/pi2Export.cjs",
    "coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "test:prepare": "node prepare-tests.js",
    "test:scraper": "node run-tests.js --scrapers",
    "test:scheduler": "node run-tests.js --scheduler",
    "test:api": "node run-tests.js --api",
    "test:websocket": "node run-tests.js --websocket",
    "test:all": "node run-tests.js --all",
    "test:clean": "node run-tests.js --all --cleanup",
    "test:launcher": "node test-launcher.js",
    "scraper:zillow": "node run-zillow-direct.js",
    "scraper:auction": "node run-auction-direct.js",
    "test:api-real": "node test-api-real.js",
    "worker": "tsx src/workerBootstrap.ts",
    "queue:test": "tsx src/scripts/enqueueTestJob.ts",
    "queue:build": "tsc --project tsconfig.queue.json --noEmit",
    "server:integrated": "node scripts/start-integrated.mjs",
    "server:windows": "node scripts/start-windows.mjs",
    "kill:windows": "powershell -ExecutionPolicy Bypass -File scripts/kill-node-windows.ps1",
    "kill:windows:simple": "./scripts/kill-node-windows.bat",
    "restart:windows": "npm run kill:windows && timeout /t 2 /nobreak && npm run server:windows",
    "ops:stack:up": "docker compose up -d",
    "ops:prom:restart": "docker compose restart prometheus",
    "ops:warm": "node scripts/smoke/warmMetrics.mjs",
    "ops:prom:check": "node scripts/ops/checkProm.mjs",
    "ops:dashboard:notes": "echo Import ops/dashboards/convexa.json in Grafana, pick Prometheus DS, set Last 30m + 10s refresh.",
    "ops:staging:smoke": "node scripts/runStagingSmoke.cjs",
    "ops:staging:seed": "node scripts/seedStagingLeades.cjs",
    "ops:backup": "node scripts/ops/backup.mjs",
    "ops:generate-redaction-sample": "node scripts/ops/generateRedactionSample.mjs",
    "hygiene:audit": "node scripts/audit/routesInventory.mjs; node scripts/audit/deadCodeAnalysis.mjs; node scripts/audit/dupeFinder.mjs; node scripts/audit/envUsage.cjs; node scripts/audit/schemaDump.cjs; node scripts/audit/metricsLogging.cjs",
    "hygiene:verify": "node scripts/ci/verifyHygieneGates.cjs",
    "ci:prep": "tsc --project tsconfig.server.json && mkdir -p backend/data ops/findings",
    "ci:test": "npm run smoke:pi1:all && npm run smoke:pi2:all && vitest run --coverage",
    "ci:hygiene": "npm run hygiene:audit && npm run hygiene:verify"
  },
  "keywords": [
    "real-estate",
    "ai",
    "lead-generation",
    "empire-builder",
    "probate",
    "foreclosure",
    "intelligence"
  ],
  "author": "Empire Builder",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^10.0.0",
    "@prisma/client": "^5.22.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/json2csv": "^5.0.7",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19.1.12",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^19.1.9",
    "@types/twilio": "^3.19.2",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.12.2",
    "better-sqlite3": "^12.2.0",
    "bullmq": "^5.12.4",
    "cheerio": "^1.1.0",
    "commander": "^14.0.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.4",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.1",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.19.0",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.6",
    "openai": "^4.52.7",
    "postcss": "^8.5.6",
    "prom-client": "^15.1.2",
    "puppeteer": "^24.16.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.8.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^4.1.13",
    "twilio": "^5.10.1",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "winston-mail": "^2.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/basic-auth": "^1.1.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitest/coverage-v8": "^2.1.9",
    "cross-env": "^10.0.0",
    "eslint": "^8.53.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "knip": "^5.64.0",
    "pg": "^8.16.3",
    "prisma": "^5.22.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "tsx": "^4.6.2",
    "typescript": "^5.2.2",
    "vitest": "^2.1.9",
    "yaml": "^2.8.1",
    "zod": "^3.25.76"
  }
}
