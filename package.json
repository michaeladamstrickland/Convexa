{
  "name": "leadflow-ai-empire",
  "version": "1.0.0",
  "description": "Revolutionary Intelligence-Driven Real Estate Empire Builder",
  "type": "module",
  "main": "dist/server.js",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "tsx src/server.ts",
    "real-server": "tsx src/realServer.ts",
    "setup": "npx prisma generate && npx prisma migrate dev --name init && npx tsx prisma/seed.ts",
    "build": "tsc",
    "typecheck": "tsc --project tsconfig.server.json",
    "start": "node dist/server.js",
    "db:setup": "npx prisma migrate dev --name init && npx prisma generate && npx tsx prisma/seed.ts",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:reset": "npx prisma migrate reset --force",
    "db:generate": "npx prisma generate",
    "phase3": "tsc && node dist/phase3-launcher.js interactive",
    "empire-complete": "tsc && node dist/phase3-launcher.js complete",
    "empire-scaling": "tsc && node dist/phase3-launcher.js phase3",
    "empire-dashboard": "tsc && node dist/phase3-launcher.js dashboard",
    "empire-report": "tsc && node dist/phase3-launcher.js report",
    "empire-autonomous": "tsc && node dist/phase3-launcher.js autonomous",
    "empire-status": "tsc && node dist/phase3-launcher.js status",
  "test": "jest --detectOpenHandles",
    "test:unit": "vitest run --reporter=dot",
    "test:contract": "vitest run tests/contract --reporter=dot",
    "test:smoke": "vitest run tests/smoke --reporter=dot",
  "test:routes": "vitest run -t routes --reporter=default",
  "test:smoke:tiny": "node scripts/smoke/dialerTiny.mjs",
    "coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "test:prepare": "node prepare-tests.js",
    "test:scraper": "node run-tests.js --scrapers",
    "test:scheduler": "node run-tests.js --scheduler",
    "test:api": "node run-tests.js --api",
    "test:websocket": "node run-tests.js --websocket",
    "test:all": "node run-tests.js --all",
    "test:clean": "node run-tests.js --all --cleanup",
    "test:launcher": "node test-launcher.js",
    "scraper:zillow": "node run-zillow-direct.js",
    "scraper:auction": "node run-auction-direct.js",
    "test:api-real": "node test-api-real.js",
    "worker": "tsx src/workerBootstrap.ts",
    "queue:test": "tsx src/scripts/enqueueTestJob.ts",
    "queue:build": "tsc --project tsconfig.queue.json --noEmit",
    "server:integrated": "node scripts/start-integrated.mjs",
    "ops:stack:up": "docker compose up -d",
    "ops:prom:restart": "docker compose restart prometheus",
    "ops:warm": "node scripts/smoke/warmMetrics.mjs",
    "ops:prom:check": "node scripts/ops/checkProm.mjs",
    "ops:dashboard:notes": "echo Import ops/dashboards/convexa.json in Grafana, pick Prometheus DS, set Last 30m + 10s refresh.",
    "ops:staging:smoke": "node scripts/smoke/stagingSmoke.mjs"
  },
  "keywords": [
    "real-estate",
    "ai",
    "lead-generation",
    "empire-builder",
    "probate",
    "foreclosure",
    "intelligence"
  ],
  "author": "Empire Builder",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/json2csv": "^5.0.7",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19.1.12",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^19.1.9",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.12.2",
    "better-sqlite3": "^12.2.0",
    "bullmq": "^5.12.4",
    "cheerio": "^1.1.0",
    "commander": "^14.0.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.1",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.19.0",
    "morgan": "^1.10.1",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "openai": "^4.52.7",
    "postcss": "^8.5.6",
    "prom-client": "^15.1.2",
    "puppeteer": "^24.16.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.8.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^4.1.13",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^8.53.0",
    "pg": "^8.16.3",
    "prisma": "^5.22.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "tsx": "^4.6.2",
    "typescript": "^5.2.2",
    "vitest": "^2.1.9",
    "zod": "^3.25.76"
  }
}
